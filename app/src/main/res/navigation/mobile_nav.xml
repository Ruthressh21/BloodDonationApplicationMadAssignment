<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_nav"
    app:startDestination="@id/donorHome">

    <fragment
        android:id="@+id/donorHome"
        android:name="my.edu.latestblooddonationapp.donorHome"
        android:label="fragment_donor_home"
        tools:layout="@layout/fragment_donor_home" >
        <action
            android:id="@+id/action_donorHome_to_editProfileFragment2"
            app:destination="@id/editProfileFragment2" />
    </fragment>
    <fragment
        android:id="@+id/editProfileFragment2"
        android:name="my.edu.latestblooddonationapp.editProfileFragment"
        android:label="fragment_edit_profile"
        tools:layout="@layout/fragment_edit_profile" >
        <action
            android:id="@+id/action_editProfileFragment2_to_donorHome"
            app:destination="@id/donorHome" />
    </fragment>
</navigation>